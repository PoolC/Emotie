{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@public/*": ["public/*"],
      "@common/*": ["src/component/common/*"],
      "@pages/*": ["src/component/pages/*"],
      "@hoc/*": ["src/hoc/*"],
      "@hooks/*": ["src/hooks/*"],
      "@image/*": ["src/image/*"],
      "@store/*": ["src/store/*"],
      "@utils/*": ["src/utils/*"]
